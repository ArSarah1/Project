pipeline {
    agent { label 'agent1' }
    stages {
stage("GIT") {
    steps {
        echo "Getting Project from Git"
        git branch: "master",
            url: "https://github.com/ArSarah1/Project.git"
    }
}
        stage('Maven') {
            steps {
                sh 'mvn --version'
            }
        }
stage('Coverage Test') {
            steps {
                script {
                    sh 'mvn clean test -P test jacoco:report'
                }
            }
        }
stage('deploy Artifact') {
    steps {
        sh 'mvn deploy -DskipTests'

    }
}
   }     
}
