pipeline {
    agent { label 'agent1' }
    stages {
stage("GIT") {
    steps {
        echo "Getting Project from Git"
        git branch: "master",
            url: "https://github.com/ArSarah1/Project.git"
    }
}
        stage('Maven') {
            steps {
                sh 'mvn --version'
            }
        }
   }     
}
